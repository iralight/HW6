<h1>Sign Up</h1>
<%= form_for User.new do |f| %>
	<%=f.label :fname, 'First Name' %>
	<%=f.text_field :fname %>
<br/><br/>
	<%=f.label :lname, 'Last Name' %>
	<%=f.text_field :lname %>
<br/><br/>
	<%=f.label :email %>
	<%=f.text_field :email %>
<br/><br/>
	<%=f.label :password  %>
	<%=f.password_field :password %>
<br/><br/>
	<%=f.submit :create %>
<% end %>
